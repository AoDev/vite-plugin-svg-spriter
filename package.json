{
  "name": "vite-plugin-svg-spriter",
  "version": "0.0.1",
  "description": "Build a svg sprite from individual svgs and inject it in your root html with vite",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AoDev/vite-plugin-svg-spriter.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src --quiet && tsc --noEmit",
    "build": "del-cli lib && tsc",
    "test": "jest",
    "prepare": "NODE_ENV=production && npm run build",
    "tag-release": "standard-version"
  },
  "keywords": [
    "vite",
    "plugin",
    "svg-sprite"
  ],
  "author": "Kevin Purnelle",
  "license": "ISC",
  "dependencies": {
    "svg-sprite": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "29.5.1",
    "@types/node": "18.16.3",
    "@types/svg-sprite": "0.0.34",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "del-cli": "5.0.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.5.0",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.0",
    "typescript": "4.9.5",
    "vite": "4.3.4"
  }
}
